{
	"$schema": "https://schemas.wp.org/wp/7.0/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#3678b9",
					"name": "Primary",
					"slug": "custom-primary"
				}
			]
		},
		"layout": {
			"contentSize": "768px",
			"wideSize": "1280px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "2X-Small",
					"size": "0.25rem",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "0.5rem",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "min(0.75rem, 1vw)",
					"slug": "40"
				},
				{
					"name": "Medium",
					"size": "min(1.5rem, 2vw)",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "min(2.25rem, 3vw)",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "min(3rem, 4vw)",
					"slug": "70"
				},
				{
					"name": "2X-Large",
					"size": "min(4.5rem, 6vw)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"fluid": true,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "0.875rem"
					},
					"name": "Medium",
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.75rem",
						"min": "1.25rem"
					},
					"name": "Large",
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2.25rem",
						"min": "1.5rem"
					},
					"name": "Extra Large",
					"size": "2.25rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3.25rem",
						"min": "2rem"
					},
					"name": "Extra Extra Large",
					"size": "3.25rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "4.5rem",
						"min": "2.5rem"
					},
					"name": "Extra Extra Extra Large",
					"size": "4.5rem",
					"slug": "xxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0",
				"left": "var(--wp--preset--spacing--60)",
				"right": "var(--wp--preset--spacing--60)",
				"top": "0"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}
