{
	"$schema": "https://schemas.wp.org/wp/7.0/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#3678b9",
					"name": "Primary",
					"slug": "custom-primary"
				}
			]
		},
		"layout": {
			"contentSize": "768px",
			"wideSize": "1280px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "2X-Small",
					"size": "0.25rem",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "0.5rem",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "min(0.75rem, 1vw)",
					"slug": "40"
				},
				{
					"name": "Medium",
					"size": "min(1.5rem, 2vw)",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "min(2.25rem, 3vw)",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "min(3rem, 4vw)",
					"slug": "70"
				},
				{
					"name": "2X-Large",
					"size": "min(4.5rem, 6vw)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"spacing": {
			"padding": {
				"bottom": "0",
				"left": "2rem",
				"right": "2rem",
				"top": "0"
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}